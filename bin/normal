#!/bin/zsh

tmux new-session -s Om -n nnn -d
tmux send-keys -t Om 'nnn -s a' C-m
tmux new-window -n ViM -t Om
tmux send-keys -t Om 'vim' C-m
tmux new-window -n bash -t Om
tmux send-keys -t Om 'bash' C-m
tmux new-window -n zsh -t Om
tmux send-keys -t Om 'source ~/.zshenv' C-m
tmux select-window -t Om:1
tmux attach -t Om
